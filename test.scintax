// A demonstration of multi level inheritance in scintax
function fib(num){
	if(num < 2) return num;
	return fib(num - 1) + fib(num - 2);
}

class Fib{
	Fib(num){ this.num = num; }
	solve(){ return fib(this.num); }
}

class FibWriter : Fib {
	FibWriter(num){ super(num); }
	solve(){ print("The solution is: " + base.solve() + "\n"); }
}

class MultiLevelInheritanceTest : FibWriter {
	MultiLevelInheritanceTest(num){ super(num); }
	solve(){ base.solve(); print("This is being called\n"); }
}

var solver = MultiLevelInheritanceTest(10);
solver.solve();

